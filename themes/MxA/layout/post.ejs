<!-- Post Laytout (post.ejs) -->
<body class="post">
  <main>

    <article>

      <h1><%- page.title %></h1>

      <% if (page.youtube) { %>
        <%- partial('_partial/embed-youtube', {item: page}) %>
      <% } else { %>
        <%- partial('_partial/post-image', {item: page}) %>
      <% } %>

      <%- page.content %>

    </article>
  </main>
</body>